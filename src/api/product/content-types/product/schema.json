{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "PricePerBundle": {
      "type": "decimal"
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Enable": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "Colors": {
      "type": "component",
      "repeatable": true,
      "component": "products.color",
      "required": true,
      "min": 1
    },
    "Category2": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category2.category2",
      "inversedBy": "Products"
    },
    "Feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "Product"
    },
    "Lengths": {
      "type": "component",
      "repeatable": true,
      "component": "products.length"
    },
    "Texture": {
      "type": "component",
      "repeatable": false,
      "component": "products.texture"
    },
    "Quality": {
      "type": "component",
      "repeatable": false,
      "component": "products.quality"
    },
    "Origin": {
      "type": "component",
      "repeatable": false,
      "component": "products.origin"
    },
    "Standards": {
      "type": "component",
      "repeatable": true,
      "component": "products.standard"
    }
  }
}
